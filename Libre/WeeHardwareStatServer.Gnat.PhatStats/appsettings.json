{
  "Logging": {
    "LogLevel": {
      "Default": ""
    }
  },
  "OutputSettings": {
    "OutputInterval": 3200,
    "CustomOutput": true,

    //Gnat-Stats & Phat-Stats Compatiable String
    "CustomOutputFormat": "C{CpuTemperature}c {CpuLoad}%|G{GpuTemperature}c {GpuLoad}%|R{RamUsed}GB|RA{RamAvailable}|RL{RamLoad}|GMT{GpuMemoryTotal}|GMU{GpuMemoryUsed}|GML{GpuMemoryLoad}|GFANL{GpuFanSpeedLoad}|GRPM{GpuFanSpeedRpm}|GPWR{GpuPower}|CPU:{CpuName}GPU:{GpuName}|GCC{GpuCoreClock}|GMC{GpuMemoryClock}|GSC{GpuShaderClock}|CHC{CpuClock}|"

  },
  "SerialPortSettings": {
    "Port": "COM9",  //Change to your Specific Arduino port
    "BaudRate": 9600,
    "Parity": "None",
    "DataBits": 8,
    "StopBits": "One",
    "ReadTimeout": 500,
    "WriteTimeout": 500,
    "DtrEnable": true,
    "RtsEnable": true
  }
}